{
  "name": "myccc",
  "version": "1.0.0",
  "description": "Cycling Calories Calculator - Cloudflare Workers Edition",
  "main": "src/index.ts",
  "scripts": {
    "dev": "wrangler dev",
    "deploy": "wrangler deploy",
    "start": "wrangler dev",
    "build": "wrangler deploy --dry-run",
    "db:create": "wrangler d1 create cycling-data",
    "db:init": "wrangler d1 execute cycling-data --local --file=./schema.sql",
    "db:init:remote": "wrangler d1 execute cycling-data --remote --file=./schema.sql",
    "test": "vitest"
  },
  "keywords": [
    "cycling",
    "calories",
    "gpx",
    "elevation",
    "fitness",
    "tracking",
    "weather",
    "cloudflare",
    "workers",
    "serverless"
  ],
  "author": "Haiku Zen",
  "license": "MIT",
  "dependencies": {
    "axios": "^1.6.0",
    "googleapis": "^161.0.0",
    "hono": "^4.5.0",
    "nodemailer": "^7.0.6",
    "vitest": "^3.2.4",
    "xml2js": "^0.6.2"
  },
  "devDependencies": {
    "@cloudflare/workers-types": "^4.20240925.0",
    "@types/nodemailer": "^7.0.2",
    "@types/xml2js": "^0.4.14",
    "typescript": "^5.9.2",
    "wrangler": "^4.40.3"
  },
  "engines": {
    "node": ">=18.0.0"
  }
}
